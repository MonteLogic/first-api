{
	"apiVersion": 2,
	"name": "first-api/checkout-newsletter-subscription",
	"version": "2.0.0",
	"title": "Newsletter Subscription!",
	"category": "first-api",
	"description": "Adds a newsletter subscription checkbox to the checkout.",
	"supports": {
		"html": false,
		"align": false,
		"multiple": false,
		"reusable": false
	},
	"parent": [ "woocommerce/checkout-contact-information-block" ],
	"attributes": {
		"lock": {
			"type": "object",
			"default": {
				"remove": true,
				"move": true
			}
		},
		"text": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-first-api-checkout-newsletter-subscription",
			"default": ""
		}
	},
	"textdomain": "first-api",
	"editorStyle": "file:../../../build/style-first-api-checkout-newsletter-subscription-block.css"
}
